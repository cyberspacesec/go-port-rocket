<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2015/06/nuspec.xsd">
  <metadata>
    <id>go-port-rocket</id>
    <version>$version$</version>
    <title>Go Port Rocket</title>
    <authors>cyberspacesec</authors>
    <projectUrl>https://github.com/cyberspacesec/go-port-rocket</projectUrl>
    <licenseUrl>https://github.com/cyberspacesec/go-port-rocket/blob/main/LICENSE</licenseUrl>
